{
    "name": "scl",
    "displayName": "SCL",
    "description": "Structured Conversation Language",
    "version": "0.0.11",
    "publisher": "daanrutten",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "scl",
                "aliases": [
                    "Structured Conversation Language",
                    "scl"
                ],
                "extensions": [
                    ".scl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "scl",
                "scopeName": "source.scl",
                "path": "./syntaxes/scl.tmLanguage"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/daanrutten/scl-syntax.git"
    }
}
